source 'https://rubygems.org'

# Specify your gem's dependencies in foreplay.gemspec
gemspec

group :development do
  gem 'bundler'
  gem 'gem-release'
  gem 'guard'
  gem 'guard-rspec'
  gem 'guard-rubocop'
  gem 'rubocop'
end

group :test do
  gem 'coveralls'
  gem 'fuubar'
  gem 'rspec'
  gem 'rspec_junit_formatter'
  gem 'simplecov', '~> 0.13'
end
